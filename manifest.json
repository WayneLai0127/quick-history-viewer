{
	"manifest_version" : 2,
	"name" : "historyTracker",
	"description" : "Extension to track history on current url",
	"version" : "1.2",
	// "background": {
 //        "page": "background.html"
	// },	
	"browser_action": {
		"default_title" : "historyTracker",
		"default_icon" : "icon.png",
		"default_popup" : "popup.html"
	},
	"commands" : {
		"shortcut-key to activate popup.html" : {
			"suggested_key" : {"default" : "Alt+1"},
			"description" : "activate popup",
			"default_popup" : "popup.html"
		}
	},
	// "background" : {
	// 	"scripts" : ["popup_script.js"],
	// 	"persistent" : false
	// },
	"icons" : {
		//"16" : "icon16.png",
		//"48" : "icon48.png",
		"128" : "icon128.png"
	},
	"permissions" : [
		"tabs",
		"history"
	]
}
