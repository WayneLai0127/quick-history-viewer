{
	"manifest_version" : 3,
	"name" : "historyTracker",
	"description" : "Extension to track history on current url",
	"version" : "1.2",	
	"action": {
		"default_title" : "historyTracker",
		"default_icon" : "icon.png",
		"default_popup" : "popup.html"
	},
	"commands" : {
		"shortcut-key to activate popup.html" : {
			"suggested_key" : {"default" : "Alt+1"},
			"description" : "activate popup",
			"default_popup" : "popup.html"
		}
	},
	"icons" : {
		"128" : "icon128.png"
	},
	"permissions" : [
		"tabs",
		"history"
	]
}
